package kgh;

import java.util.Scanner;

public class KGH {

	public static void main(String[] args) {

		int[] 배열 = new int[5];
		
		Scanner sc = new Scanner(System.in);
		
		int 엿 = 0;
		while(엿<배열.length) {
			System.out.println("값을 입력하세요!");
			배열[엿] = sc.nextInt();
			엿++;
		}
		
		for(int 영원=0; 영원<배열.length; 영원++) {
			System.out.println(배열[영원]);
			
		}
 		
		
		
		
		
	}

}
